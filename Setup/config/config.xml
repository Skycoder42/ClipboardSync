		<Repository>
			<Url>https://install.skycoder42.de/installers/ClipboardSync</Url>
			<Enabled>1</Enabled>
			<DisplayName>Clipboard-Sync Online Repository</DisplayName>
		</Repository>
	</RemoteRepositories>
	<ControlScript>controllerScript.js</ControlScript>
</Installer>
